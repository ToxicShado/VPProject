<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
	  <!-- What should these values be?-->
	  <add key="V_threshold" value="3.5" />
    <add key="Z_threshold" value="0.05" />
    <add key="DeviationPercent" value="25" />
  </appSettings>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="NetTcpBinding_EIS"
                 transferMode="Streamed"
                 maxReceivedMessageSize="102400" 
                 openTimeout="00:01:00"
                 closeTimeout="00:01:00"
                 receiveTimeout="00:10:00"
                 sendTimeout="00:10:00">
          <readerQuotas maxArrayLength="102400" maxStringContentLength="102400"/>
        </binding>
      </netTcpBinding>
    </bindings>
    <services>
      <service name="Server.BatteryServer">
        <endpoint address=""
                  binding="netTcpBinding"
                  bindingConfiguration="NetTcpBinding_EIS"
                  contract="Common.IBatteryCommands" />
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:25565/BatteryServer"/>
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
</configuration>